<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="parallelexecution" parallel="tests" thread-count="2">
	<test name="segment1">
		<classes>
			<class
				name="com.trello.qspiders.learntestng.DataDrivenTestingByClass"></class>
			<class
				name="com.trello.qspiders.learntestng.UsageOfBeforeAndAfterMethod"></class>
		</classes>
	</test> <!-- Test -->
	<test name="segment2">
		<classes>
			<class
				name="com.trello.qspiders.learntestng.DependsOnMethodsCheck"></class>
			<class
				name="com.trello.qspiders.learntestng.DisableTestCaseFromExecution"></class>
		</classes>
	</test>
</suite> <!-- Suite -->
